@model List<t2210m.Models.UserRegisterModel>
@{
    //var ls = ViewData["users"];
    var ls = ViewBag.users;
}
<div class="controller">
    <h1>Danh sách người dùng (số lượng : @Model.Count)</h1>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Email</th>
            <th>FullName</th>
            <th>Telephone</th>
        </tr>
    </thead>
    <tbody>
      @foreach(t2210m.Models.UserRegisterModel item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Email</td>
                    <td>@item.FullName</td>
                    <td>@item.Telephone</td>
                </tr>
            }
        </tbody>
    </table>
</div>